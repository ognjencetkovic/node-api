{
  "root": true,
  "parser": "@typescript-eslint/parser",
  "plugins": [
    "@typescript-eslint"
  ],
  "extends": [
    "eslint:recommended",
    "plugin:@typescript-eslint/eslint-recommended",
    "plugin:@typescript-eslint/recommended",
    "plugin:prettier/recommended"
  ],
  "rules": {
    "no-console": 1,
    "@typescript-eslint/ban-types": [
      "error",
      {
        "types": {
          "Function": false,
          "object": false
        },
        "extendDefaults": true
      }
    ],
    "no-irregular-whitespace": 1
  }
}